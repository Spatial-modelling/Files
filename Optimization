require(gurobi)
install.packages("prioritizr", repos = "https://cran.rstudio.com/")
library(prioritizr)

#Build module
p1 <- problem(PUs4, features = spsR,cost_column = "AR") %>%
  add_min_set_objective() %>%
  add_relative_targets(0.15) %>%
  add_binary_decisions() %>%
  add_default_solver(gap = 0)

